<urlrewrite>
    <rule>
        <note>
            商品页面的伪静态
        </note>
        <from>^/(.*_p)([1234567890]{1,}).html</from>
        <to type="forward">/home/pdt_PdtProduct_gtProductsInfo?id=$2</to>
    </rule>
    <rule>
        <note>
            资源文件
        </note>
        <from>^/static/*</from>
        <to type="redirect">/home/static/$1</to>
    </rule>
    <rule>
        <note>
            商品页面
        </note>
        <from>^/pdt_PdtProduct_gtProductsInfo?id=([1234567890]{1,})</from>
        <to type="redirect">/home/pdt_PdtProduct_gtProductsInfo?id=$1</to>
    </rule>

    <rule>
        <note>
            罗马尼亚店铺资源文件
        </note>
        <from>^/m/romania/(.*).css</from>
        <to type="redirect">/home/$1.css</to>
    </rule>
    <rule>
        <note>
            罗马尼亚店铺js文件
        </note>
        <from>^/m/romania/(.*).js</from>
        <to type="redirect">/home/$1.js</to>
    </rule>
    <rule>
        <note>
            罗马尼亚店铺
        </note>
        <from>^/m/romania[/]$</from>
        <to type="forward">/home/usr_UsrSupplier_gtSupIndex?pkey=262</to>
    </rule>

