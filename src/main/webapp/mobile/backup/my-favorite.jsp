            $.ajax({
                type: 'post',
                url: 'http://' + window.location.host + '/home/usr_UsrFavorites_del?pkey=' + pkey,
                dataType: 'json',
                success: function (data) {
                    console.log(data);
                    if (data.success == true) {
                        alert("删除成功");
                        location.reload();
                    } else {
                        alert("删除失败");
